  <div class="deliveryBeforeCut0ff" style="visibility:hidden; height: 0" data-beforecutoff="{{ settings.beforeCutOff | times: 1 }}">{{ settings.beforeCutOff | times: 1 }}</div>
  <div class="deliveryAfterCutOff" style="visibility:hidden; height: 0" data-aftercutoff="{{ settings.afterCutOff | times: 1 }}">{{ settings.afterCutOff | times: 1 }}</div>

