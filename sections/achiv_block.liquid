<style>
  .list_two{
    padding-left: 50px!important;
  }
  .img img{
    width: 100%;
  }
  .img1 img, .img2 img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 8px;
    overflow: hidden;
  }
  .achiv_block{
    margin: auto;
  }
  .achiv_head{
    width: 100%;
    display: grid;
    grid-template-rows: auto;
    grid-template-columns: 1fr 1fr;
  }
  .achiv_list{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 30px 80px 30px 165px;
  }
  .achiv_list h4{
    font-family: Graphik-Regular;
    font-weight: normal;
    font-size: 32px;
    line-height: 42px;
  }
  .achiv_item p{
    font-family: Graphik-Regular;
    font-weight: normal;
    font-size: 16px;
    line-height: 26px;
    color: #6C6E73;
  }
  .achiv_item{
    display: flex;
    flex-direction: row;
  }
  .achiv_item_text{
    display: flex;
    flex-direction: column;
  }
  .achiv_block h1{
    font-family: Graphik-Regular;
    font-weight: normal;
    font-size: 32px;
    line-height: 42px;
    padding-left: 165px;
  }
  .achiv_item h4{
    font-family: Graphik-Regular;
    font-weight: normal;
    font-size: 32px;
    line-height: 42px;
  }
  .achiv_item_icon{
    padding-right: 46px;
  }
  .middle_item{
    padding-top: 80px;
    padding-bottom: 80px;
  }
  .svg_mobile{
    width: 82px;
    height: 82px;
  }
  .achiv_info{
    display: flex;
    max-width: 433px;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
  }
  .achiv_info a{
    font-size: 16px;
    line-height: 20px;
    color: #FF4438;
    border-bottom: 2px solid;
  }
  .achiv_info a:hover{
    border-bottom: none;
  }
  @media screen and (max-width: 1600px){
    .middle_item{
      padding-top: 18px;
      padding-bottom: 18px;
    }
    .achiv_list h4{
      margin-bottom: 0;
    }
  }

  @media screen and (max-width: 1250px){
    .achiv_block h1{
      padding-left: 80px;
    }
    .achiv_list{
      padding-left: 80px;
      padding-right: 80px;
    }
    .achiv_item h4{
      font-weight: normal;
      font-size: 25px;
