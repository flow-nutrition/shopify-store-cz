      color: #222222;
    }
    hr:not([size]){
      display: none;
    }
    .site-footer__hr{
      border: none;
      margin: 0;
    }
    .site-footer__copyright-content{
      margin: 0;
    }
    .copyright-items{
      flex-direction: column-reverse;
      text-align: center;
    }
    .footer-container{
      border-top: #E9ECEE 1px solid;
      border-bottom: #E9ECEE 1px solid;
      padding: 40px 0;
    }
    .site-footer__logo-link svg{
      width: 165px;
      height: 18px;
    }
    .footer-logo{
      margin: 0;
    }
    .footer-bottom-wrapper{
      padding: 8px 16px 0 16px;
    }
    .time-block{
      margin-bottom: 20px;
    }
    .space-bellow{
      display: none;
    }
    .site-footer-top-content{
      padding: 0 16px;
    }
    .footer-bottom-wrapper{
      justify-content: space-between;
    }
    .site-footer__content{
      padding: 0 16px;
    }
    .site-footer{
      padding: 40px 0 0 0;
    }
    .site-footer-top-content{
      flex-direction: column;
    }
    .time-block{
      width: 100%;
      flex-direction: column;
    }
    .contacts-block{
      flex-wrap: wrap;
    }
    .contact-item, .social-icon{
      margin-bottom: 15px;
    }
  }

  @media (max-width: 768px) {
  .bottom-links-wrapper .footer__section{
    display: flex;
    flex-direction: column;
  }
    .menu-items-wrapper{
      margin-left: 0px;
      margin-right: 0px;
    }
    .site-footer__content{
      padding-top: 0;
      flex-direction: column;
    }
    .site-footer__item:first-of-type{}

    .site-footer__item{
      padding-top: 24px;
      padding-bottom: 24px;
      border-bottom: 1px solid #E9ECEE;
      margin-bottom: 0;
      width: 100%;
    }
    .site-footer .h4{
      margin-bottom: 0;
    }
    .site-footer__item-inner.site-footer__item-inner--link_list{
      width: 100%;
    }
    .site-footer__item:not(:last-child) .site-footer__linklist .site-footer__linklist-item:first-of-type{
      margin-top: 32px;
    }

    .h4.site-footer-block-title:after {
      content: url("{{ "dropdown-arrow.svg" | asset_url }}");
      position: absolute;
      top: calc(50% - 10px);
      right: 0;
    }
    .h4.site-footer-block-title.visible:after {
      content: url("{{ "dropdown-arrow.svg" | asset_url }}");
      transform: rotate(180deg);
      font-size: 15px;
      position: absolute;
      top: calc(50% - 10px);
      right: 0;
    }
    .site-footer-block-content{
      display: none;
    }
    .site-footer__item:last-of-type .site-footer-block-content{
      display: block!important;
    }
    .site-footer__item:last-of-type .h4.site-footer-block-title{
      display: none!important;
    }
    .site-footer__item:last-of-type{
      padding: 0;
      border: none;
    }
    .site-footer__item:last-of-type .site-footer__linklist{
      display: flex!important;
      flex-wrap: wrap;
      margin-top: 32px;
      justify-content: space-between;
    }
    .site-footer__item:last-of-type .site-footer__linklist-item{
      width: 35%;
      padding: 0 0px 24px 0;
    }
  }
</style>

<script>


  if ($(window).width() < 749) {
    var titleTrigger = $('.site-footer-block-title');
    titleTrigger.click(function (){
      $(this).next('.site-footer-block-content').slideToggle(250);
      if ( $(this).hasClass('visible')){
        $(this).removeClass('visible');
      } else {
        // $('.site-footer-block-title').removeClass('visible');
        if ($('.site-footer-block-title ').not(this).hasClass('visible')){
          $('.site-footer-block-title.visible ').not(this).next('.site-footer-block-content').slideToggle(250);
          $('.site-footer-block-title ').not(this).removeClass('visible');

        }

        $(this).addClass('visible');
      }
    });
  }



</script>

